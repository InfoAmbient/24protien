<config>
    <modules>
        <M3_Ismsshipment>
            <version>0.0.1</version>
        </M3_Ismsshipment>
    </modules>
    <global>
        <models>
            <sslwismsshipment>
                <class>Ismsshipment_Model</class>
            </sslwismsshipment>
        </models>
        <events>
            <sales_order_shipment_save_after>
                <observers>
                    <M3_Ismsshipment_model_observer>
                        <type>singleton</type>
                        <class>M3_Ismsshipment_Model_Observer</class>
                        <method>SendSmsOrderShipment</method>
                    </M3_Ismsshipment_model_observer>
                </observers>
            </sales_order_shipment_save_after> 
        </events>
    </global>
</config>
